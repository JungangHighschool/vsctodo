#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/vstodo/vstodo/node_modules/.pnpm/@vscode+test-cli@0.0.6/node_modules/@vscode/test-cli/out/node_modules:/workspaces/vstodo/vstodo/node_modules/.pnpm/@vscode+test-cli@0.0.6/node_modules/@vscode/test-cli/node_modules:/workspaces/vstodo/vstodo/node_modules/.pnpm/@vscode+test-cli@0.0.6/node_modules/@vscode/node_modules:/workspaces/vstodo/vstodo/node_modules/.pnpm/@vscode+test-cli@0.0.6/node_modules:/workspaces/vstodo/vstodo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/vstodo/vstodo/node_modules/.pnpm/@vscode+test-cli@0.0.6/node_modules/@vscode/test-cli/out/node_modules:/workspaces/vstodo/vstodo/node_modules/.pnpm/@vscode+test-cli@0.0.6/node_modules/@vscode/test-cli/node_modules:/workspaces/vstodo/vstodo/node_modules/.pnpm/@vscode+test-cli@0.0.6/node_modules/@vscode/node_modules:/workspaces/vstodo/vstodo/node_modules/.pnpm/@vscode+test-cli@0.0.6/node_modules:/workspaces/vstodo/vstodo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@vscode/test-cli/out/bin.mjs" "$@"
else
  exec node  "$basedir/../@vscode/test-cli/out/bin.mjs" "$@"
fi
